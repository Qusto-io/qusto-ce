# Whitelist for git-secrets false positives
# These are test/development keys that are safe to commit

# Test environment SECRET_KEY_BASE (not real secrets)
config/.env.dev:.*SECRET_KEY_BASE=
config/.env.load:.*SECRET_KEY_BASE=
config/.env.test:.*SECRET_KEY_BASE=

# Test database URLs
config/.env.test:.*DATABASE_URL=

# Template/example files â€” placeholder values, not real secrets
.env.example:.*SECRET_KEY_BASE=
DEPLOYMENT.md:.*SECRET_KEY_BASE=
docker-compose.deploy.yml:.*SECRET_KEY_BASE=
